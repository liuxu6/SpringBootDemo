<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.liuxu.service.mapper.UserMapper">
    <insert id="addUser">
        insert into user(user_id,user_code,user_name,password,state) values (#{userId},#{userCode},#{userName},#{password},#{state});
    </insert>

</mapper>